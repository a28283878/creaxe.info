<!DOCTYPE html>
@Scripts.Render("~/bundles/jquery")
<html>
<head>
    <!-- Bootstrap Core JavaScript -->
    <script src="@Url.Content("~/Scripts/bootstrap.min.js")"></script>
    <!-- Contact Form JavaScript -->
    <script src="@Url.Content("~/Scripts/jqBootstrapValidation.js")"></script>
    <!-- Theme JavaScript -->
    <!-- Bootstrap Core CSS -->
    <link rel="stylesheet" href="@Url.Content("~/Content/Ntnu/style.css")">
    <!-- Resource style -->
    <link href="@Url.Content("~/Content/bootstrap.min.css")" rel="stylesheet">
    <!-- Theme CSS -->
    <link href="@Url.Content("~/Content/clean-blog.min.css")" rel="stylesheet">
    <!-- Custom Fonts -->
    <link href="@Url.Content("~/Content/font-awesome.min.css")" rel="stylesheet" type="text/css">
    <link href="@Url.Content("~/Content/Ntnu/info.css")" rel="stylesheet">
</head>
@{
    @Html.Partial("_NavPartial");
}
<body>

    <div class="row" style="width:100%; height:50px;"></div>

    <main class="cd-main-content animsition">
        <div class="cd-fixed-bg cd-bg-1">
            <div class="gray-shadow">
                <h2 style="text-align:center; font-weight:800; top:35%;">學程介紹</h2>
                <hr style="height:2px;border:none;background-color:white; margin-top:60px; top:35%;">
                <span class="fa fa-chevron-down down" style="color:white; top:55%; font-size:2.5em;" aria-hidden="true"></span>
            </div>
        </div>
        <!-- cd-fixed-bg -->
        <div class="cd-scrolling-bg">
            <div class="container" id="here">
                <div class="row">
                    <div class="wow col-md-5 fadeInDown">
                        <h3>學程使命</h3>
                        <hr style="height:1px;border:none;background-color:#aaa;">
                        <h4 style="margin-top:35px; line-height:2em;">
                            為鼓勵學生嘗試多元跨領域學系，將各系所之專業帶進實務創業。並透過小組實踐與討論的過程中，碰撞出全新創意火花，激發無限創新、創業可能。
                        </h4>
                    </div>
                    <div class="col-md-7 wow fadeInDown" style="vertical-align:middle;">
                        <h3>課程影片</h3>
                        <hr style="height:1px;border:none;background-color:#aaa;">
                        <div class="videoWrapper" style="margin-top:35px;   padding-top:0px;">
                            <video class="videoSize" src="~/img/Ntnu/大師創業學程_宣傳影片.mp4" controls="controls"></video>
                            @*<iframe width="854" height="480" src="https://www.youtube.com/embed/2HDpTIMg-Pw" frameborder="0" allowfullscreen></iframe>*@
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="wow col-sm-12 fadeInUp" style="text-align: center; margin-top:40px;">
                        <a class="btn animsition-link" style="display: inline-block; text-align: center;" href="/Ntnu/index">最新消息</a>
                    </div>
                </div>
            </div>
        </div>
        <div class="cd-fixed-bg cd-bg-2">
            <div class="gray-shadow">
                <h2 style="text-align:center; font-weight:800;  top:35%;">學程特色</h2>
                <hr style="height:2px;border:none;background-color:white; margin-top:60px;  top:35%;">
                <span class="fa fa-chevron-down down" style="color:white; top:55%; font-size:2.5em;" aria-hidden="true"></span>
            </div>
        </div>
        <!-- cd-fixed-bg -->
        <div class="cd-scrolling-bg" style="width:100%; background-color:#f8f8f8">
            <div class="container">
                <div class="row">
                    <div class="wow col-xs-8 col-xs-offset-2 fadeInDown" style="text-align:center">
                        <h2>學程特色</h2>
                    </div>
                    <hr class="wow col-xs-4 col-xs-offset-4 fadeInUp" style="padding:0;height:1px;border:none;color:#aaa;background-color:#333;">
                </div>
                <div style="margin-bottom:20px;"></div>
                <div class="row">
                    <img src="~/img/ntnu/feature.jpg" style="width:100%; height:auto; box-shadow:3px 3px 15px"/>
                </div>
                <div class="row">
                    <div class="wow col-sm-12 fadeInUp" style="text-align: center; margin-top:40px;">
                        <a class="btn animsition-link" style="display: inline-block; text-align: center;" href="/Ntnu/index">最新消息</a>
                    </div>
                </div>
            </div>
        </div>
        <div class="cd-fixed-bg cd-bg-3">
            <div class="gray-shadow">
                <h2 style="text-align:center; font-weight:800;  top:35%;">重要日程</h2>
                <hr style="height:2px;border:none;background-color:white; margin-top:60px;  top:35%;">
                <span class="fa fa-chevron-down down" style="color:white; top:55%; font-size:2.5em;" aria-hidden="true"></span>
            </div>
        </div>
        <!-- cd-fixed-bg -->
        <div class="cd-scrolling-bg">

            <div style="width:100%;">
                <section id="cd-timeline" class="cd-container" style=" margin-top:0;">
                    <div class="cd-timeline-block">
                        <div class="wow cd-timeline-img cd-picture zoomIn">
                            <i class="fa fa-paint-brush fa-lg"></i>
                        </div>
                        <!-- cd-timeline-img -->
                        <div class="wow cd-timeline-content fadeInDown">
                            <h2>創業企劃成果發表會</h2>
                            <p></p>
                            <span class="cd-date">6/24</span>
                        </div>
                        <!-- cd-timeline-content -->
                    </div>
                    <!-- cd-timeline-block -->
                    <div class="cd-timeline-block">
                        <div class="wow cd-timeline-img cd-movie zoomIn">
                            <i class="fa fa-wrench fa-lg"></i>
                        </div>
                        <!-- cd-timeline-img -->
                        <div class="wow cd-timeline-content fadeInDown">
                            <h2>創業經營教戰手則工作坊</h2>
                            <p></p>
                            <span class="cd-date">8/28-9/1</span>
                        </div>
                        <!-- cd-timeline-content -->
                    </div>                
                </section>
                <!-- cd-timeline -->
            </div>
            <footer>
                <div class="container">
                    <div class="col-xs-12">
                        <hr style="width: 100%; border-top: 1px solid #ccc; margin-top:50px;">
                        <span class="copyright text-muted">Richard 2017</span>
                    </div>
                </div>
            </footer>
        </div>
    </main>
</body>
</html>
<script src="https://use.typekit.net/wyd4emv.js"></script>
<script>
    try {
        Typekit.load({
            async: false
        });
    } catch (e) { }
</script>